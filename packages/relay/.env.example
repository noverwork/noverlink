# Noverlink Relay Configuration
# REQUIRED: Copy this file to .env and set all values

# WebSocket port for CLI connections
# Production: 443 or 8443 | Development: 8444
WS_PORT=8444

# HTTP port for public traffic
# Production: 80 or 8080 | Development: 8080
HTTP_PORT=8080

# Base domain for tunnel URLs
# Production: noverlink.io | Development: localhost:8080
BASE_DOMAIN=localhost:8080

# Log level: error, warn, info, debug, trace
# Production: info | Development: debug
RUST_LOG=info
