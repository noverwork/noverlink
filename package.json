{
  "name": "@noverlink/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "backend:serve": "nx serve @noverlink/backend",
    "backend:build": "nx build @noverlink/backend",
    "frontend:serve": "nx dev @noverlink/frontend",
    "frontend:build": "nx build @noverlink/frontend",
    "ui-shared:shadcn": "cd packages/ui-shared && npx shadcn@latest",
    "relay:serve": "nx serve @noverlink/relay",
    "relay:build": "nx build @noverlink/relay",
    "cli:serve": "nx serve @noverlink/cli",
    "cli:build": "nx build @noverlink/cli",
    "ms:start": "./scripts/start-ms.sh",
    "ms:stop": "./scripts/stop-ms.sh",
    "ms:restart": "npm run ms:stop && npm run ms:start",
    "ms:reset": "npm run ms:stop -- --delete && npm run ms:start",
    "typecheck": "nx run-many --target=typecheck --all --exclude=source",
    "typecheck:affected": "nx affected --target=typecheck",
    "lint": "nx run-many --target=lint --all --exclude=source",
    "lint:affected": "nx affected --target=lint",
    "test": "nx run-many --target=test --all --exclude=source --output-style=stream ",
    "test:affected": "nx affected --target=test",
    "format": "prettier . -c --ignore-unknown",
    "prepare": "husky"
  },
  "private": true,
